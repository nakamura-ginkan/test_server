extends ../_layouts/default

block vars
  -title        = '下層_データなし'
  -description  = ''
  -keywords     = ''
  -pageID       = 'detail'
  -path         = '..'
  -imgPC        = path+'/img/'+pageID+"/"
  -imgSP        = path+'/img/'+pageID+"/sp/"

block container
  include ../_parts/common-nav
  .main-content
    .main-header
      .header-inner.is-detail
        a.back-button(href="#") 戻る
        h1.header-title 優待券
          span.instruction-icon.button-tooltip.js-button-tooltip(data-tooltip="tooltip-coupon")
        a.download-button.tablet-fixed(href="#")
          span CSVダウンロード
    .block-ticket-graph.js-tabs
      .graph-header
        .calendar-input-group
          .calendar-input-wrapper
            input.calendar-input.font-din.js-open-modal.has-wrapper(data-modal-desktop="modal-calendar" data-modal="modal-term" type="text" value="2020/08/01")
          span.calendar-symbol -
          .calendar-input-wrapper
            input.calendar-input.font-din.js-open-modal.has-wrapper(data-modal-desktop="modal-calendar" data-modal="modal-term" type="text" value="2020/09/01")
          a.calendar-button.js-open-modal.is-toggle(href="#" data-modal="modal-term") 期間
        ul.tabs-group.js-list-tabs(data-selector="tab-container")
          li.tab-item.is-active(data-tab="total") 合計
          li.tab-item(data-tab="regular") 常連プログラム
          li.tab-item(data-tab="customers") SynchroLife送客
      #total.tab-container.is-active
        .tabs-group-wrapper
          ul.tabs-group.is-sub.js-list-tabs(data-selector="tab-all")
